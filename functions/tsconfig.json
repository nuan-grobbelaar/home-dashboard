{
	"compilerOptions": {
		"lib": ["es2020"],
		"module": "commonjs",
		"target": "es2020",
		"outDir": "lib",
		"esModuleInterop": true,
		"strict": true,
		"sourceMap": true,
		"rootDir": "src",
		"types": ["node"],
		"jsx": "react-jsx"
	},
	"compileOnSave": false,
	"include": ["src"],
	"exclude": ["node_modules"]
}
